    <test verbose="2" preserve-order="true" name="Google Search with Keywords for Images">
        <classes>
            <class name="org.impero.testcases.GoogleSearchTest">
                <methods>
                    <include name="verifyGoogleSearchForImages"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>